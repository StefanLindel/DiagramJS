{
  "name": "DiagramJS",
  "version": "0.0.3",
  "description": "DiagramJS",
  "license": "EUPL-1.1",
  "repository": {
    "type": "git",
    "url": "https://gitlab.se.cs.uni-kassel.de/slin/DiagramJS.git"
  },
  "dependencies": {
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "npm-watch": "^0.1.6",
    "rimraf": "^2.5.4",
    "rollup": "^0.34.13",
    "rollup-plugin-commonjs": "^4.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "tslint": "^3.15.1",
    "typescript": "^1.8.10",
    "uglify-js-harmony": "^2.6.2"
  },
  "babel": {
    "presets": ["es2015"]
  },
  "watch": {
    "build-dev": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "ts",
      "quiet": true
    }
  },
  "scripts": {
    "build-dev": "tsc && rollup -m -c || true",
    "build-prod": "rimraf build && tsc && rollup -c && rimraf build/tmp && babel build -d build && uglifyjs ./build/diagram.js -o ./build/diagram.min.js -c || true",
    "build-watch": "npm-watch"
  }
}
